
class Demo1{
	String name;
	int age=28;
	void disp() { 
		System.out.println("Demo 1"+age+" "+name);
	}
}


class Demo2 extends Demo1
{
  
	void dis() {
	name="Hello";
  }
	
}

class Demo3 extends Demo2
{
	
}

public class LaunchInheritance1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Demo1 d=new Demo1();
		d.disp();
		
		

	}

}
