

class Employeee{
	 String eid1;
	 String ename1;
	 String eaddrr1;
	public Employeee(String eid, String ename, String eaddrr) {
		super();
		this.eid1 = eid1;
		this.ename1 = ename1;
		this.eaddrr1 = eaddrr1;
	}
	
}


// Target Obj
class Department{
	private String did;
	private String dname;
	// HAS-A Variable 
	private Employee[] emp;
	public Department(String did, String dname, Employee[] emp) {
		super();
		this.did = did;
		this.dname = dname;
		this.emp = emp;
	}
	
	public void getDepartmentDetails() {
		System.out.println("Department Details !!");
		System.out.println("-------------");
		System.out.println(" Department Id : "+did);
		System.out.println(" Department Name : "+dname);
		System.out.println("$$$$$$$$$$$$$$$$$$$$$$$$$");
		System.out.println(" Employee details are  ");
		System.out.println("------------------");
		for( Employee employee:emp) {
			System.out.println(" Employee id ::" + employee.eid);
			System.out.println("Employee name :: "+employee.ename);
			System.out.println("Employee address :: "+employee.eaddrr);
			
		}
	}
	
	
}
public class OneToManyAssociation {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
