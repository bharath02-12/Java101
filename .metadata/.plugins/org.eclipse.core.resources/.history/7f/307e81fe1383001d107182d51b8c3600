



class Calci{
	int a=10 ;
	int b=20;
	int result ;
	
	int  sum()
	{
		result =a+b;
		
		return result;
	}
	
	int mul() {
		result=a*b;
		return result;
	}
}

public class Launchcalc1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Calci calc=new Calci();
		int ans=calc.sum();
		System.out.println(ans);
		System.out.println();

	}

}
