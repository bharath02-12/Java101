



class Calci{
	int a ;
	int b;
	int result ;
	
	int  sum()
	{
		result =a+b;
		
		return result;
	}
	
	int mul() {
		result=a*b;
		return result;
	}
}

public class Launchcalc1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Calci calc=new Calci();
		calc.sum();

	}

}
