
class Dog1{
	int cost;
	String name;
	
	
	public int getCost() {
		return cost;
	}
	public String getName() {
		return name;
	}
	public Dog1() {
		this("Sheru",99);
	}
	public Dog1(String name, int cost) {
		this.name=name;
		this.cost=cost;
	}
	
}
public class Launchencapdog {
	public static void main(String[] args) 
	{
		Dog1 d1= new Dog1();
		System.out.println(d1.getCost());
		System.out.println(d1.getName());
		
		Dog1 d2 = new Dog1("Robo",100);
		System.out.println(d1.getCost());
		System.out.println(d1.getName());
		
		
	}

}
