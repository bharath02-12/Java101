class Student{
	int age;//--> Instacnce variable// data members 
	String name;
	String city;
	
	void setAge(int a) {
		age=a;
	}
	
	int getAge() {
		return age;
	}
	
	void setName(String s) {
		name=s;
	}
	
	String getName() {
		return name;
	}
	
	void setCity(String s) {
		city=s;
	}
	
	String getCity() {
		return city;
	}
}

public class LaunchEncap {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Student st = new Student();
		st.age=28;
		st.name="Hyder";
		st.city="Bangaluru";
		st.setName("Hyderabad");

	}

}
