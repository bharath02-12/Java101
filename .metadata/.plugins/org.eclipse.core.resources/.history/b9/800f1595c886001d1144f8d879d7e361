
public class Loops {
	
	public static void main(String[] args) {
		
		int a=10;
		int b=20;
		int c=a+b;
		System.out.println(c);
		int d =10;
		System.out.println(d);
		
		for( int i=0;i++<5;i++) {
			System.out.println("i = ");
			System.out.println("Hello !!");
			i=i++;
			System.out.println(i);
		}
		
	}
}
