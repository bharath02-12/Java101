import java.util.Scanner;

public class Arrays {

	public static void main(String[] args) {
		
		Scanner sc= new Scanner(System.in);
		
		int []a[]= {{10,20},{30,40,50}};
		
		System.out.println(a.getClass().getName());
		
		for( int arr[]:a)
		{
			
			
		for( int ele :arr) 
		{
			System.out.print(ele+" ");
		}
		
		System.out.println(" ");

	    }
	}

}
