
class Student1{
	private String name;
	private int age;
	private String city;
	
	
	 public Student1(String name,int age,String city) {
		 this.age=age;
	 this.name=name;
	 this.city=city; }
	 
	 
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public String getCity() {
		return city;
	}
	
	
}

public class LaunchStud {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Student1 st1 = new Student1("hyder",28,"pune");
		

	}

}
